import os
# assign directory
directory = '/home/tcosendey/code/Tfcosendey/hungry_birds/Images'
my_files = []
# iterate over files in
# that directory
for filename in os.listdir(directory):
    f = os.path.join(directory, filename)
    # checking if it is a file
    if os.path.isfile(f):
        my_files.append(f)

print(my_files)
